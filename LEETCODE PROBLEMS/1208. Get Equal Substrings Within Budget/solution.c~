// COMPLETED: 3ms RUNTIME; 10:37 AM 3-22-24


int equalSubstring(char* s, char* t, int maxCost) {
	int currentCost = 0; 
	int maxLength = 1; 
	for (int i = 0; i < strlen(s); i ++) {
		if (s[i] == t[i]) {
			currentCost = currentCost + 0; 
		}
		else {
			if (s[i] - t[i] > 0) {
				currentCost = currentCost+s[i]-t[i];
			}
			
			else {
				currentCost=currentCost+t[i]-s[i]; 
			}

			if (currentCost > maxCost) {
				return i; 
			}
		}
		maxLength = i+1; 

	
	}



	return maxLength;


}
